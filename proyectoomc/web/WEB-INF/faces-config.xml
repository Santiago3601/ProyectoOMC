<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <locale-config>
            <default-locale>es</default-locale>
            <supported-locale>es</supported-locale>
            <supported-locale>en</supported-locale>
        </locale-config>
        
        <resource-bundle>
            <base-name>language.RegistrarPermisoLaboral-es</base-name>
            <var>msjRegistrarPermisoLaboral</var>
        </resource-bundle>
        
        <resource-bundle>
            <base-name>language.ActualizarPermisoLaboral-es</base-name>
            <var>msjActualizarPermisoLaboral</var>
        </resource-bundle>
        
        <resource-bundle>
            <base-name>language.App</base-name>
            <var>msjApp</var>
        </resource-bundle>
        
        <resource-bundle>
            <base-name>language.RegistrarTurno-es</base-name>
            <var>msjRegistrarTurno</var>
        </resource-bundle>
        
        <resource-bundle>
            <base-name>language.ActualizarTurno-es</base-name>
            <var>msjActualizarTurno</var>
        </resource-bundle>
        
        <resource-bundle>
            <base-name>language.RegistrarPunto-es</base-name>
            <var>msjRegistrarPunto</var>
        </resource-bundle>
        
        <resource-bundle>
            <base-name>language.ActualizarPunto-es</base-name>
            <var>msjActualizarPunto</var>
        </resource-bundle>
        
        <resource-bundle>
            <base-name>language.RegistrarHorario-es</base-name>
            <var>msjRegistrarHorario</var>
        </resource-bundle>
        
         <resource-bundle>
            <base-name>language.ListarPunto-es</base-name>
            <var>msjListarPunto</var>
        </resource-bundle>
        
    </application>
</faces-config>
